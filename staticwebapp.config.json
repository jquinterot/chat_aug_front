{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/*.css", "/*.js", "/*.json"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  }
}
